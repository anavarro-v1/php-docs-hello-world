# Azure Storage Configuration
# Copy this file to .env and fill in your Azure Storage credentials

# Your Azure Storage connection string
# Get this from Azure Portal > Storage Account > Access Keys
# Format: DefaultEndpointsProtocol=https;AccountName=<account_name>;AccountKey=<account_key>;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONNECTION_STRING=

# Container name for storing documents (optional, default: documents)
AZURE_STORAGE_CONTAINER=documents
